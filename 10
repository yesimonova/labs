#include <stdio.h>
#include <math.h>

int main()
{
    printf("Выберите номер подзадания.\n");
    int answ;
    scanf ("%d",&answ);
    switch (answ) {
        case 1:
            printf("Это задание на проверку истинности высказывания.\n");
            int A,B;
            printf("Введите число A.\n");
            scanf("%d",&A);
            printf("Введите число B.\n");
            scanf("%d",&B);
            if (A>2 && B<=3) {
                printf("Неравенства A>2 и B=<3 справедливы.\n");
            }
            else {
                printf("Неравенства A>2 и B=<3 несправедливы.\n");
            }
            break;
        case 2:
            printf("Это задание на проверку истинности высказывания.\n");
            int A1,B1,C1;
            printf("Введите число A.\n");
            scanf("%d",&A1);
            printf("Введите число B.\n");
            scanf("%d",&B1);
            printf("Введите число C.\n");
            scanf("%d",&C1);
            if (A1<B1 && B1<C1) {
                printf("Неравенство A<B<C справедливо.\n");
            }
            else {
                printf("Неравенство A<B<C несправедливо.\n");
            }
            break;
        case 3:
            printf("Это задание на проверку истинности высказывания.\n");
            int x;
            printf("Введите число X.\n");
            scanf("%d",&x);
            if (x%2==0 && x>9 && x<100) {
                printf("Высказывание о том, что данное число является чётным двузначным, истинно.\n");
            }
            else {
                printf("Высказывание о том, что данное число является чётным двузначным, ложно.\n");
            }
            break;
        case 4:
            printf("Это задание на проверку истинности высказывания.\n");
            int y,y1,y2,y3;
            printf("Введите трёхзначное число Y.\n");
            scanf("%d",&y);
            y1=y/100;
            y2=y%100/10;
            y3=y%10;
            if ((y1<y2 && y2<y3)||(y1>y2 && y2>y3)) {
                printf("Высказывание о том, что цифры данного числа образуют возрастающую или убывающую последовательность, истинно.\n");
            }
            else {
                printf("Высказывание о том, что цифры данного числа образуют возрастающую или убывающую последовательность, ложно.\n");
            }
            break;
        case 5:
            printf("Это задание на проверку истинности высказывания.\n");
            int z,z1,z2,z3,z4;
            printf("Введите четырёхзначное число Z.\n");
            scanf("%d",&z);
            z1=z/1000;
            z2=z%1000/100;
            z3=z%100/10;
            z4=z%10;
            if (z1==z4 && z2==z3) {
                printf("Высказывание о том, что данное число читается одинаково слева направо и справа налево, истинно.\n");
            }
            else {
                printf("Высказывание о том, что данное число читается одинаково слева направо и справа налево, ложно.\n");
            }
            break;
        case 6:
            printf("Это задание на проверку истинности высказывания.\n");
            int a,b,c;
            printf("Введите сторону треугольника a.\n");
            scanf("%d",&a);
            printf("Введите сторону треугольника b.\n");
            scanf("%d",&b);
            printf("Введите сторону треугольника c.\n");
            scanf("%d",&c);
            if ((a*a+b*b==c*c)||(b*b+c*c==a*a)||(a*a+c*c==b*b)) {
                printf("Высказывание о том, что треугольник со сторонами a, b, c является прямоугольным, истинно.\n");
            }
            else {
                printf("Высказывание о том, что треугольник со сторонами a, b, c является прямоугольным, ложно.\n");
            }
            break;
        case 7:
            printf("Это задание на проверку истинности высказывания.\n");
            int a1,b1,c1;
            printf("Введите сторону треугольника a.\n");
            scanf("%d",&a1);
            printf("Введите сторону треугольника b.\n");
            scanf("%d",&b1);
            printf("Введите сторону треугольника c.\n");
            scanf("%d",&c1);
            if ((a1+b1)>c1 && (b1+c1)>a1 && (a1+c1)>b1) {
                printf("Высказывание о том, что треугольник с заданными сторонами a,b,c существует, истинно.\n");
            }
            else {
                printf("Высказывание о том, что треугольник с заданными сторонами a,b,c существует, ложно.\n");
            }
            break;
        default:
            printf("Неверный номер подзадания.");
    }
    return 0;
}
