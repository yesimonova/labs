#include <stdio.h>
#include <math.h>

int main()
{
    printf("Выберите номер подзадания.\n");
    int answ;
    scanf ("%d",&answ);
    switch (answ) {
        case 1:
            printf("Это задание на вывод чисел.\n");
            int A,B;
            printf("Введите число целое положительное число A (A<B).\n");
            scanf("%d",&A);
            printf("Введите число целое положительное число B (A<B).\n");
            scanf("%d",&B);
            for (int i=B-1;i>A;i--) {
                for (int j=0;j<i;j++) {
                    printf("%d ",i);
                }
            }
            break;
        case 2:
            printf("Это задание на нахождение незанятой части отрезка.\n");
            int A1,B1;
            printf("Введите число целое положительное число A (A>B).\n");
            scanf("%d",&A1);
            printf("Введите число целое положительное число B (A>B).\n");
            scanf("%d",&B1);
            while ((A1-B1)>=0) {
              A1=A1-B1;
            }
            printf("Длина незанятой части равна %d",A1);
            break;
        case 3:
            printf("Это задание на нахождение наименьшего из целых чисел, для которых сумма 1 + 2 + . . . + K будет больше или равна N.\n");
            int N,K=0,sum=0;
            printf("Введите целое положительное число, которое больше единицы.\n");
            scanf("%d",&N);
            while (sum<=N) {
              K++;
              sum+=K;
            }
            printf("Наименьшее из целых чисел K, для которых сумма 1 + 2 + . . . + K больше или равна N равно %d\n",K);
            printf("Сумма равна %d\n",sum);
            break;
        case 4:
            printf("Это задание на нахождение размера вклада через определённое количество месяцев.\n");
            double S1=1000,P1,K1=0;
            printf("Введите вещественное число P, обозначающее процент (0<P<25).\n");
            scanf("%lf",&P1);
            while (S1<=1100) {
              K1++;
              S1+=S1*P1/100;
            }
            printf("Количество месяцев равно %lf\n",K1);
            printf("Размер вклада будет равен %lf\n",S1);
            break;
        case 5:
            printf("Это задание на нахождение НОДа двух целых положительных чисел .\n");
            int A2,B2;
            printf("Введите первое целое положительное число.\n");
            scanf("%d",&A2);
            printf("Введите второе целое положительное число.\n");
            scanf("%d",&B2);
            while (A2!=0 && B2!=0) {
              if (A2>B2) {
                A2%=B2;
              }
              else {
                B2%=A2;
              }
            }
            printf("НОД равен %d",A2+B2);
            break;
        case 6:
            printf("Это задание на нахождение порядкового номера числа Фибоначчи.\n");
            int N2,K2=2,c=0,c1=1,c2=1;
            printf("Введите целое положительное число N, которое больше единицы.\n");
            scanf("%d",&N2);
            while (c<=N2) {
              K2++;
              c=c1+c2;
              c2=c1;
              c1=c;
            }
            printf("Порядковый номер числа Фибоначчи N равен %d",K2);
            break;
        default:
            printf("Неверный номер подзадания.");
    }
    return 0;
}
